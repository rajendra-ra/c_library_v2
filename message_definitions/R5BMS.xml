<?xml version="1.0"?>
<mavlink>
  <version>3</version>
  <!-- <include>minimal.xml</include> -->
  <messages>
    <message id="11069" name="BMS_AUX_DATA">
      <description>Data from auxillary microcontroller and BMS microcontroller</description>
      <field name="aux_current" type="uint16_t">aux_battery_current</field>
      <field name="aux_voltage" type="uint16_t">aux_battery_voltage</field>
      <field name="total_current" type="uint16_t">total current</field>
      <field name="total_voltage" type="uint16_t">total voltage</field>
      <field name="esc1_current" type="uint16_t">esc1 current</field>
      <field name="esc2_current" type="uint16_t">esc2 current</field>
      <field name="bat1_temperature" type="uint16_t">battery1 temperature</field>
      <field name="bat2_temperature" type="uint16_t">battery2 temperature</field>
      <field name="esc1_temperature" type="uint16_t">esc1 temperature</field>
      <field name="esc2_temperature" type="uint16_t">esc2 temperature</field>
      <field name="motor1_temperature" type="uint16_t">motor1 temperature</field>
      <field name="motor2_temperature" type="uint16_t">motor2 temperature</field>
      <field name="errorcode_cout" type="uint16_t">fault detection data:cout</field>
      <field name="errorcode_dout" type="uint16_t">fault detection data:dout</field>
    </message>

    <message id="11071" name="TOP_MID_PCB_DATA">
      <description>Data from top and mid pcb controllers microcontroller</description>
      <field name="total_voltage" type="uint16_t">top battery voltage</field>
      <field name="servo1_current" type="uint16_t">top servo1 current</field>
      <field name="servo2_current" type="uint16_t">top servo2 current</field>
      <field name="servo3_current" type="uint16_t">top servo3 current</field>
      <field name="servo1_temperature" type="uint16_t">top servo1 temperature</field>
      <field name="servo2_temperature" type="uint16_t">top servo2 temperature</field>
      <field name="servo3_temperature" type="uint16_t">top servo3 temperature</field>
      <field name="extra1" type="uint16_t">ambient temperature 1</field>
      <field name="extra2" type="uint16_t">ambient temperature 2</field>
      <field name="mid_servo1_current" type="uint16_t">mid_servo1_current</field>
      <field name="mid_servo2_current" type="uint16_t">mid_servo2_current</field>
      <field name="mid_servo3_current" type="uint16_t">mid_servo3_current</field>
      <field name="mid_servo1_temperature" type="uint16_t">mid servo1 temperature</field>
      <field name="mid_servo2_temperature" type="uint16_t">mid servo2 temperature</field>
      <field name="mid_servo3_temperature" type="uint16_t">mid servo3 temperature</field>
      <field name="bec1_voltage" type="uint16_t">bec1 voltage</field>
      <field name="bec2_voltage" type="uint16_t">bec2 voltage</field>
      <field name="buck_voltage" type="uint16_t">buck voltage</field>
    </message>
  </messages>
</mavlink>
